{
  "reporter": "junit",
  "reporterOptions": {
    "mochaFile": "cypress/reports/junit/results-[hash].xml",
    "toConsole": true
  },
  "env": {
    "browserPermissions": {
      "notifications": "allow"
    }
  },
  "numTestsKeptInMemory": 1,
  "experimentalSourceRewriting": true,
  "screenshots": true,
  "video": true,
  "viewportWidth": 1920,
  "viewportHeight": 1080,
  "defaultCommandTimeout": 10000
}